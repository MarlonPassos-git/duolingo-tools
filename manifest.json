{
  "$schema": "http://json.schemastore.org/chrome-manifest",
  "manifest_version": 3,
  "name": "Meu Plugin Duolingo",
  "version": "1.0",
  "description": "Adiciona um quadrado fixo na tela que diz 'ol√° mundo' quando clicado.",
  "content_scripts": [
    {
      "matches": [
        "https://www.duolingo.com/*"
      ],
      "js": [
        "index.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "styles.css"
      ],
      "matches": [
        "https://www.duolingo.com/*"
      ]
    }
  ]
}